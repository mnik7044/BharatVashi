generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id           Int     @id @default(autoincrement())
  email        String  @unique
  password     String
  name         String?
  aadharNumber String  @unique
  profileUrl   String
  phoneNumber  String  @unique
}

model Post {
  id            Int      @id @default(autoincrement())
  contentString String
  postUserId    Int
  name          String
  profileImgUrl String
  createdAt     DateTime @default(now())
}
